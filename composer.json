{
    "name": "tapansharma/breeze-svelte",
    "description": "Minimal Laravel authentication scaffolding with Svelte and Tailwind.",
    "keywords": [
        "laravel",
        "auth",
        "breeze-svelte"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tapan288/breeze-svelte/issues",
        "source": "https://github.com/tapan288/breeze-svelte"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Tapan Sharma",
            "email": "tapansharma69@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^11",
        "illuminate/filesystem": "^11",
        "illuminate/support": "^11",
        "illuminate/validation": "^11"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10"
    }
}